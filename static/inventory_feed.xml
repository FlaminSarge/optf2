$def with (user, pack)
<?xml version="1.0" encoding="UTF-8"?>

<rss version="2.0">
  <channel>
    <title>$user.get_persona()'s TF2 Backpack</title>
    <generator>$product_name</generator>
    <link>${instance.homedomain}${virtual_root}</link>
  $ items = pack.get_items()
  $items.reverse()
  $for item in items:
      <item>
        $ permalink = "{0}{1}item/{2}/{3}".format(instance.homedomain, virtual_root, user.get_id64(), pack.get_item_id(item))
        <description>Level $pack.get_item_level(item) $pack.get_item_type(item)</description>
        <title>$qualitydict[pack.get_item_quality(item)["str"]] $pack.get_item_name(item)</title>
        <link>$permalink</link>
        <guid isPermaLink="true">$permalink</guid>
      </item>
  </channel>
</rss>
